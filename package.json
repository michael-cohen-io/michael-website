{
  "name": "michael-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prismaMigrateLocal": "dotenv -e .env.local -- pnpm dlx prisma migrate dev && dotenv -e .env.local -- pnpm dlx prisma generate",
    "prismaMigrateLocalNoSeed": "dotenv -e .env.local -- pnpm dlx prisma migrate dev --skip-seed && dotenv -e .env.local -- pnpm dlx prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@vercel/postgres": "^0.5.1",
    "classnames": "^2.3.2",
    "next": "14.0.3",
    "react": "^18",
    "prisma": "^5.6.0",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "prettier": "3.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}